	up        ---|  QX2.0_0 X

048	QX2.4 = f0;

	f0        ---@  QX2.4


	f0        ---|  QX2.4_0 X

049
050	QB1   = d0;				// QB1 =

	IB1     A ---@  QB1     A


	IB1     A ---+  QB1_0   W       IB1     // 1

051	imm int a;				// a   =
052	QX2.5 = D(~QX2.5, c1 = CLOCK(b1 = CHANGE(a = d0/5)));

	IB1     A ---+  a       A       IB1     // 1
	                                /5;     // (2)


	b1_1    v --->  b1

	iClock  : ---+  b1_1    v
	a       A ---+                  a       // 1


	c1_1    C ---:  c1      :

	iClock  : ---|  c1_1    C
	b1        ---|


	QX2.5_1 D ---#  QX2.5

	c1      : ---|  QX2.5_1 D
	QX2.5   ~ ---|


	QX2.5     ---|  QX2.5_0 X

053
054	QX2.6 = (IX2.0 & IX2.1 & IX2.2) |
055	        (IX2.3 & IX2.4 & IX2.5) ;

	QX2.6_1   ---|  QX2.6
	QX2.6_2   ---|

	IX2.0     ---&  QX2.6_1
	IX2.1     ---&
	IX2.2     ---&

	IX2.3     ---&  QX2.6_2
	IX2.4     ---&
	IX2.5     ---&


	QX2.6     ---|  QX2.6_0 X

056	QX2.7 = IX2.6 ^ IX2.7 |
057		a < 10;

	QX2.7_1   ---|  QX2.7
	QX2.7_2   ---|

	IX2.6     ---^  QX2.7_1
	IX2.7     ---^

	a       A ---+  QX2.7_2         a       // 1
	                                <10;    // (3)


	QX2.7     ---|  QX2.7_0 X

058	QX2.3 = D(IX2.0, t, 10);

	QX2.3_1 D ---#  QX2.3

	t       ! ---|  QX2.3_1 D
	10      A<---|
	IX2.0     ---|


	QX2.3     ---|  QX2.3_0 X

