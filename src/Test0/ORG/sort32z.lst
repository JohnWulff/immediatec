037	/********************************************************************
038	 *
039	 *	Segment
040	 *
041	 *	Each segment controls one gate and may be up to 32 steps long
042	 *
043	 *******************************************************************/
044
045	imm bit segment(		/* returns gate control output */
046		imm int carryIn,	/* shift bit (as int) from the previous segment */
047	 assign	imm int carryOut,	/* shift bit (as int) for the following segment */
048		imm int code,		/* code identifying this segment */
049		imm int length,		/* segment length */
050		imm int width,		/* width of the gate */
051		imm clock c,		/* stepping clock for the belt */
052	    )
053	{
054	    extern imm bit reset;	/* general re-initialisation */
055	    imm int shift = SHR((shift << 1) + carryIn, c, reset);

	segment@shift_1 H ---*  segment@shift   A
	segment@shift_2 r ---*

	segment@c       : ---+  segment@shift_1 H
	segment@shift_3 A ---+                          segment@shift_3      // 1
	segment@shift   A ---+                          ?segment@shift       // 2
	segment@shift_4 A ---+                          :segment@shift_4     // 3
	                                                ;                    // (1)

	iClock          : ---|  segment@shift_2 r
	IX0.7             ---|

	IX0.7             ---|  segment@shift_3 A

	segment@shift   A ---+  segment@shift_4 A       (segment@shift       // 1
	segment@carryIn A ---+                          <<1)+segment@carryIn // 2
	                                                ;                    // (6)

056	    imm int mask  = 0x41 << width;

	segment@width A ---+  segment@mask A       0x41<<segment@width // 1
	                                           ;                   // (3)

057	    carryOut = (shift >> length) & 1;

	segment@shift  A ---+  segment@carryOut A       (segment@shift   // 1
	segment@length A ---+                           >>segment@length // 2
	                                                )&1;             // (4)

058	    this = SRX((shift & 0x7f) == 0x41 + (code << 1),
059		       (shift & mask) == mask | reset, c);

	segment@_1    S ---#  segment@
	segment@_2    R ---#

	segment@c     : ---&  segment@_1 S
	segment@_3      ---&
	segment@_4    ~ ---&

	segment@c     : ---&  segment@_2 R
	segment@_3    ~ ---&
	segment@_4      ---&

	segment@shift A ---+  segment@_3         (segment@shift             // 1
	segment@code  A ---+                     &0x7f)==0x41+(segment@code // 2
	                                         <<1);                      // (7)

	segment@_5      ---|  segment@_4
	IX0.7           ---|

	segment@shift A ---+  segment@_5         (segment@shift             // 1
	segment@mask  A ---+                     &segment@mask              // 2
	                                         )==segment@mask            // 2
	                                         ;                          // (5)

060	}
