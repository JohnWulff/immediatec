	belt@shift3_1 H ---*  belt@shift3   A
	belt@shift3_2 r ---*

	belt@clk      : ---+  belt@shift3_1 H
	belt@shift3_3 A ---+                        belt@shift3_3        // 1
	belt@shift3   A ---+                        ?belt@shift3         // 2
	belt@shift3_4 A ---+                        :belt@shift3_4       // 3
	                                            ;                    // (1)

	iClock        : ---|  belt@shift3_2 r
	IX0.7           ---|

	IX0.7           ---|  belt@shift3_3 A

	belt@shift3   A ---+  belt@shift3_4 A       (belt@shift3         // 1
	belt@car2     A ---+                        <<1)+belt@car2       // 2
	                                            ;                    // (6)


	iConst        A ---+  belt@mask3    A       0x41<<7;             // (10)


	belt@shift3   A ---+  belt@car3     A       (belt@shift3         // 1
	                                            >>12)&1;             // (9)


	belt@gate2_1  S ---#  belt@gate2
