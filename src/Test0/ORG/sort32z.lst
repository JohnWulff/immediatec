153	    gate7 = segment(car7, car8,    7, 12,  7, clk);

	belt@shift8_1 H ---*  belt@shift8   A
	belt@shift8_2 r ---*

	belt@clk      : ---+  belt@shift8_1 H
	belt@shift8_3 A ---+                        belt@shift8_3        // 1
