116	    imm int car7;		/* carry bit: 7 */
117	    imm int car8;		/* carry bit: 8 */
118	    imm int car9;		/* carry bit: 9 */
119	    imm int car10;		/* carry bit: 10 */
120	    imm int car11;		/* carry bit: 11 */
121	    imm int car12;		/* carry bit: 12 */
122	    imm int car13;		/* carry bit: 13 */
123	    imm int car14;		/* carry bit: 14 */
124	    imm int car15;		/* carry bit: 15 */
125	    imm int car16;		/* carry bit: 16 */
126	    imm int car17;		/* carry bit: 17 */
127	    imm int car18;		/* carry bit: 18 */
128	    imm int car19;		/* carry bit: 19 */
129	    imm int car20;		/* carry bit: 20 */
130	    imm int car21;		/* carry bit: 21 */
131	    imm int car22;		/* carry bit: 22 */
132	    imm int car23;		/* carry bit: 23 */
133	    imm int car24;		/* carry bit: 24 */
134	    imm int car25;		/* carry bit: 25 */
135	    imm int car26;		/* carry bit: 26 */
136	    imm int car27;		/* carry bit: 27 */
137	    imm int car28;		/* carry bit: 28 */
138	    imm int car29;		/* carry bit: 29 */
139	    imm int car30;		/* carry bit: 30 */
140	    imm int car31;		/* carry bit: 31 */
141	    imm int car32;		/* carry bit: 32 */
142
143	    imm clock clk = CLOCK(strobe);

	belt@clk_1  C ---:  belt@clk   :

	iClock      : ---|  belt@clk_1 C
	belt@strobe   ---|

144
145	    lock   = feeder(p_cell, car0, code, 12, 11, clk);

	belt@pip_1   E ---/  belt@pip

	belt@clk     : ---&  belt@pip_1   E
	belt@p_cell    ---&
	belt@pip_2   ~ ---&
	IX0.7        ~ ---&

	belt@pip_3   S ---#  belt@pip_2
	belt@pip_4   R ---#

	belt@clk     : ---&  belt@pip_3   S
	belt@pip       ---&
	belt@pip_5   ~ ---&

	belt@clk     : ---&  belt@pip_4   R
	belt@pip     ~ ---&
	belt@pip_5     ---&

	belt@pip_6     ---|  belt@pip_5
	IX0.7          ---|

	belt@shift   A ---+  belt@pip_6           (belt@shift        // 1
	belt@mask    A ---+                       &belt@mask         // 2
	                                          )==belt@mask       // 2
	                                          ;                  // (5)


	belt@shift_1 H ---*  belt@shift   A
	belt@shift_2 r ---*

	belt@clk     : ---+  belt@shift_1 H
	belt@shift_3 A ---+                       belt@shift_3       // 1
	belt@shift   A ---+                       ?belt@shift        // 2
	belt@shift_4 A ---+                       :belt@shift_4      // 3
	                                          ;                  // (1)

	iClock       : ---|  belt@shift_2 r
	IX0.7          ---|

	IX0.7          ---|  belt@shift_3 A

	belt@shift   A ---+  belt@shift_4 A       (belt@shift        // 1
	belt@shift_5 A ---+                       <<1)+(belt@shift_5 // 2
	belt@code    A ---+                       *(0x41+(belt@code  // 3
	                                          <<1)));            // (2)

	belt@pip       ---|  belt@shift_5 A


	iConst       A ---+  belt@mask    A       0x41<<11;          // (8)


	belt@shift   A ---+  belt@car0    A       (belt@shift        // 1
	                                          >>12)&1;           // (9)


	belt@pip_2     ---@  belt@lock

146	    gate0 = segment(car0, car1,    0, 12,  7, clk);

	belt@shift1_1 H ---*  belt@shift1   A
	belt@shift1_2 r ---*

