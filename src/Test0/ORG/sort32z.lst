181	/********************************************************************
182	 *
183	 *	Generate tick
184	 *	input fast1 or fast2 cause 50 ms ticks
185	 *	else tick for every change of manual input
186	 *
187	 *******************************************************************/
188
189	imm bit tick(bit manual, bit fast1, bit fast2) {
190	    imm bit fast = fast1 | fast2;

	tick@fast1   ---|  tick@fast
	tick@fast2   ---|

191	    this = CHANGE(manual & ~fast | TX0.4 & fast);

	tick@_1     V --->  tick@

	iClock      : ---|  tick@_1 V
	tick@_2       ---|
	tick@_3       ---|

	tick@manual   ---&  tick@_2
	tick@fast   ~ ---&

	TX0.4         ---&  tick@_3
	tick@fast     ---&

192	}
