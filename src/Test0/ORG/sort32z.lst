	belt@shift2   A ---+  belt@gate1_3          (belt@shift2         // 1
	                                            &0x7f)==0x41+(1<<1); // (12)

	belt@gate1_5    ---|  belt@gate1_4
	IX0.7           ---|

	belt@shift2   A ---+  belt@gate1_5          (belt@shift2         // 1
	belt@mask2    A ---+                        &belt@mask2          // 2
	                                            )==belt@mask2        // 2
	                                            ;                    // (5)

